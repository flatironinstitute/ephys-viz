{
    "name": "{{ extension_name }}_jup",
    "version": "{{ version }}",
    "description": "{{ description }}",
    "author": "{{ author }}",
    "license": "{{ license }}",
    "keywords": [
        "jupyter",
        "widgets",
        "ipython",
        "ipywidgets",
        "jupyterlab-extension",
        "{{ extension_name }}"
    ],
    "scripts": {
        "clean": "rimraf dist/",
        "prepublish": "webpack",
        "build": "webpack",
        "watch": "webpack --watch --mode=development",
        "install-extension": "jupyter nbextension install --py --symlink --sys-prefix {{ extension_name }}_jup && jupyter nbextension enable {{ extension_name }}_jup --py --sys-prefix",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        {% for key, value in package_json.dependencies.items() -%}
        "{{ key }}": "{{ value }}",
        {% endfor -%}
        "@jupyter-widgets/base": "^1.0.0",
        "json-stable-stringify": "^1.0.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.5.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@tulipjs/eslint-config": "^1.1.1",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "style-loader": "^0.23.1",
        "url-loader": "^2.0.1",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.6"
    },
    "jupyterlab": {
        "extension": "lib/labplugin"
    }
}
